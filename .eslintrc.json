{
  "plugins": ["prettier", "vue"],
  "extends": ["prettier", "plugin:vue/essential"],
  "rules": {
    "prettier/prettier": "warn",
    "no-shadow": [
      "error",
      {
        "builtinGlobals": true
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  }
}
